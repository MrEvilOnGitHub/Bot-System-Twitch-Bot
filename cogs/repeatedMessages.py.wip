def sendMessageToChannel(channel: str, message: str):
    for i in authDetails.CHANNELS:
        if i.lower() == channel.lower():
            raise ValueError
    loop = asyncio.get_event_loop()
    loop.create_task(Bot.get_channel(channel).send(message))
